<%- include('../partials/header') %>

<div class="center-section">
  <div class="back-link">
    <a href="/months/"><i class="fas fa-arrow-left"></i> Back to All Months</a>
  </div>
  <div class="heading">
    <h2>Weeks</h2>
  </div>

  <div class="container">
    <ul>
      <% month.weeks.forEach(function(week) { %>
      <li>
        <a href="/months/<%= month._id %>/weeks/<%= week._id %>">Week of <%= week.month %> <%= week.weekdate %> <%= week.year %></a>
    
        <a class="submit-icon" href="/weeks/<%= week._id%>/edit">
      </li>
      <% }); %>
    </ul>
  </div>
</div>

  <%- include('../partials/footer') %>